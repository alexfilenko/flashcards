<h1>Все карточки</h1>
 <%= link_to 'New card', new_card_path %>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @cards.each do |card| %>
    <tr>
      <td><%= card.original_text %></td>
      <td><%= card.translated_text %></td>
      <td><%= card.review_date %></td>
      <td><%= link_to 'Все карточки' %></td>
    </tr>
  <% end %>
</table>
